import requests 
import os
from dotenv import load_dotenv
import json
load_dotenv()

course_id = 48532

token = os.getenv("ED_API_TOKEN")
headers={'Authorization': 'Bearer ' + token}
r = requests.request("GET", "https://us.edstem.org/api/courses/48532",  headers=headers)
u = r.json()
print(json.dumps(u, indent=1))
print(json.dumps(u, indent=1))
